<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Informasi_Pribadi_Admin"
    android:background="@color/white">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="63dp"
        android:layout_height="63dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="100dp"
        android:src="@drawable/fadiyah" />

    <TextView
        android:id="@+id/nama2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="110dp"
        android:layout_marginTop="105dp"
        android:fontFamily="@font/poppins"
        android:text="Admin"
        android:textColor="@color/black"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/idnama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="110dp"
        android:layout_marginTop="134dp"
        android:fontFamily="@font/poppins"
        android:text="Admin"
        android:textColor="#1E1EAE"
        android:textSize="16dp" />

    <ImageView
        android:id="@+id/btnAdminKembali"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="30dp"
        android:layout_marginStart="27dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/btn_back" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="24dp"
        android:gravity="center"
        android:fontFamily="@font/poppins"
        android:text="Profile"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txt_pengaturanakun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:text="Informasi Pribadi"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:layout_marginTop="180dp"
        android:layout_marginStart="24dp" />

    <TextView
        android:id="@+id/username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txt_pengaturanakun"
        android:layout_alignParentStart="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/poppins"
        android:text="Username"
        android:textColor="@color/black"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/adminUsername"
        android:layout_width="360dp"
        android:layout_height="52dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="258dp"
        android:background="@drawable/btnkeluar"
        android:ems="10"
        android:inputType="text"
        android:paddingStart="15dp"
        android:textColor="@color/biru_2"
        android:textSize="14sp"
        android:enabled="false"
        android:focusable="false" />

    <TextView
        android:id="@+id/txt_Pasword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="Password"
        android:allowClickWhenDisabled="false"
        android:textColor="@color/black"
        android:textSize="18dp"
        android:layout_marginTop="330dp"
        android:layout_marginStart="24dp" />

    <EditText
        android:id="@+id/adminPw"
        android:layout_width="360dp"
        android:layout_height="52dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="358dp"
        android:background="@drawable/btnkeluar"
        android:ems="10"
        android:inputType="text"
        android:paddingStart="15dp"
        android:textColor="@color/biru_2"
        android:textSize="14sp"
        android:enabled="false"
        android:focusable="false" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="370dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="420dp"
        android:layout_marginStart="30dp"
        android:text="Username dan Password tidak bisa diubah Silahkan ke Menu"
        android:textColor="@color/pink"
        android:fontFamily="@font/poppins_bold"/>
    <TextView
        android:id="@+id/txtBantu"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="441dp"
        android:layout_marginStart="175dp"
        android:text="@string/bantuan"
        android:textColor="@color/biru_2"
        android:fontFamily="@font/poppins_bold"/>


</RelativeLayout>
