<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Informasi_Pribadi_Admin"
    android:background="@color/white">
    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="63dp"
        android:layout_height="63dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="100dp"
        android:src="@drawable/fadiyah" />
    <TextView
        android:id="@+id/nama2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="110dp"
        android:layout_marginTop="105dp"
        android:fontFamily="@font/poppins"
        android:text="Piaaa"
        android:textColor="@color/black"
        android:textSize="18dp" />
    <TextView
        android:id="@+id/idnama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="110dp"
        android:layout_marginTop="134dp"
        android:fontFamily="@font/poppins"
        android:text="Admin"
        android:textColor="#1E1EAE"
        android:textSize="16dp" />
    <ImageView
        android:id="@+id/btn_back6"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="25dp"
        android:layout_marginStart="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/btn_back" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="65dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="170dp"
        android:layout_marginTop="22dp"
        android:gravity="center"
        android:fontFamily="@font/poppins"
        android:text="Profile"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txt_pengaturanakun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:text="Informasi Pribadi"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:layout_marginTop="180dp"
        android:layout_marginStart="24dp"
        />


    <TextView
        android:id="@+id/username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txt_pengaturanakun"
        android:layout_alignParentStart="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/poppins"
        android:text="username"
        android:textColor="@color/black"
        android:textSize="18dp" />
    <EditText
        android:id="@+id/usrname"
        android:layout_width="360dp"
        android:layout_height="57dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="258dp"
        android:background="@drawable/btnkeluar"
        android:textColor="@color/biru_2"
        android:text="   nama" />
    <TextView
        android:id="@+id/txt_Pasword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="Pasword"
        android:textColor="@color/black"
        android:textSize="18dp"
        android:layout_marginTop="340dp"
        android:layout_marginStart="24dp" />
    <EditText
        android:id="@+id/pw"
        android:layout_width="360dp"
        android:layout_height="57dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="368dp"
        android:background="@drawable/btnkeluar"
        android:textColor="@color/biru_2"
        android:text="   Pasword" />

    <Button
        android:id="@+id/button2"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:text="Ubah"
        android:layout_marginTop="750dp"
        android:layout_marginStart="30dp"
        android:background="@drawable/button_masuk1" />
</RelativeLayout>


