<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pengguna_Home"
    android:background="@color/white">


    <View
        android:id="@+id/rectangle_1"
        android:layout_width="335dp"
        android:layout_height="160dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="18dp"
        android:background="@drawable/kotak_pink_home" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="36dp"
        android:layout_height="24dp"
        android:layout_alignStart="@id/rectangle_1"
        android:layout_alignTop="@id/rectangle_1"
        android:layout_marginStart="275dp"
        android:layout_marginTop="24dp"
        android:src="@drawable/notif" />
    <View
        android:id="@+id/notif"
        android:layout_width="37dp"
        android:layout_height="30dp"
        android:layout_alignStart="@id/rectangle_1"
        android:layout_alignTop="@id/rectangle_1"
        android:layout_marginStart="275dp"
        android:layout_marginTop="21dp"
        android:alpha="0.3"
        android:background="@drawable/notif_coba" />

    <View
        android:id="@+id/bulat"
        android:layout_width="240.61dp"
        android:layout_height="230.24dp"
        android:layout_alignStart="@id/rectangle_1"
        android:layout_marginBottom="-50dp"
        android:background="@drawable/bulat" />


    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/rectangle_1"
        android:layout_alignTop="@+id/rectangle_1"
        android:layout_alignBottom="@+id/rectangle_1"
        android:layout_marginStart="32dp"
        android:layout_marginTop="22dp"
        android:layout_marginBottom="103dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Hey"
        android:textColor="@color/white"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="287dp"
        android:layout_height="34dp"
        android:layout_below="@+id/rectangle_1"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="23dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="107dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Data Konsultasi Anda!"
        android:textColor="@color/black"
        android:textSize="18sp" />


    <TextView
        android:id="@+id/txt_lihat"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rectangle_1"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="255dp"
        android:layout_marginLeft="261dp"
        android:layout_marginTop="199dp"
        android:layout_marginEnd="9dp"
        android:fontFamily="@font/poppins_medium"
        android:gravity="center"
        android:text="@string/lihat"
        android:textColor="@color/biru_2"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="271dp"
        android:layout_height="25dp"
        android:layout_alignStart="@+id/rectangle_1"
        android:layout_alignEnd="@+id/rectangle_1"
        android:layout_alignBottom="@+id/rectangle_1"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="79dp"
        android:fontFamily="@font/poppins"
        android:text="@string/hey_pia"
        android:textColor="@color/white"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/namauser"
        android:layout_width="186dp"
        android:layout_height="34dp"
        android:layout_alignTop="@+id/rectangle_1"
        android:layout_alignBottom="@+id/rectangle_1"
        android:layout_marginStart="10dp"
        android:layout_marginTop="22dp"
        android:layout_marginBottom="104dp"
        android:layout_toEndOf="@+id/textView5"
        android:fontFamily="@font/poppins_bold"
        android:hint="nama pengguna"
        android:textColor="@color/white"
        android:textSize="20sp" />


    <EditText
        android:id="@+id/notelepon"
        android:layout_width="286dp"
        android:layout_height="45dp"
        android:layout_alignStart="@+id/rectangle_1"
        android:layout_alignTop="@+id/rectangle_1"
        android:layout_alignEnd="@+id/rectangle_1"
        android:layout_alignBottom="@+id/rectangle_1"
        android:layout_marginStart="33dp"
        android:layout_marginTop="89dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="26dp"
        android:background="@drawable/form_daftar"
        android:ems="10"
        android:drawablePadding="15dp"
        android:drawableLeft="@drawable/search"
        android:fontFamily="@font/poppins"
        android:hint="Pencarian"
        android:inputType="text"
        android:padding="5dp"
        android:paddingStart="15dp"
        android:textColor="@color/black"
        android:textColorHint="#A3A3AE"
        android:textSize="14sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="459dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="230dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="5dp">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <androidx.cardview.widget.CardView
                android:layout_width="349dp"
                android:layout_height="200dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginBottom="15dp"
                app:cardCornerRadius="10dp"
                >
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/white">

                    <View
                        android:id="@+id/view4"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@color/biru_2"
                        >

                    </View>

                    <TextView
                        android:id="@+id/nomor_konsul"
                        android:layout_width="100dp"
                        android:layout_height="24dp"
                        android:background="@color/biru_2"
                        android:fontFamily="@font/poppins_bold"
                        android:text="Konsultasi 1"
                        android:textColor="@color/white"
                        android:textSize="14sp"
                        android:layout_marginStart="6dp"
                        android:layout_marginTop="6dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0">

                    </TextView>

                    <TextView
                        android:id="@+id/tanggal"
                        android:layout_width="80dp"
                        android:layout_height="24dp"
                        android:layout_marginStart="270dp"
                        android:layout_marginTop="6dp"
                        android:background="@color/biru_2"
                        android:fontFamily="@font/poppins"
                        android:text="12/12/2021"
                        android:textColor="@color/white"
                        android:textSize="14sp"

                       >

                    </TextView>

                    <View
                        android:layout_width="88dp"
                        android:layout_height="80dp"
                        android:background="#F2F2F2"
                        android:layout_marginTop="35dp"
                        android:layout_marginStart="262dp"
                        >

                    </View>

                    <TextView
                        android:id="@+id/btncekHasil"
                        android:layout_width="81dp"
                        android:layout_height="49dp"
                        android:layout_marginStart="265dp"
                        android:layout_marginTop="50dp"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="Cek Hasil Konsultasi"
                        android:textColor="@color/pink"
                        android:textSize="14sp"

                        >

                    </TextView>

                    <TextView
                        android:id="@+id/txt_hasil"
                        android:layout_width="230dp"
                        android:layout_height="24dp"
                        android:layout_marginStart="6dp"
                        android:layout_marginTop="58dp"
                        android:fontFamily="@font/poppins"
                        android:text="Cek Hasil Konsultasi"
                        android:textColor="@color/biru_2"
                        android:textSize="14sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.517">

                    </TextView>


                </RelativeLayout>

            </androidx.cardview.widget.CardView>
        </ScrollView>
    </LinearLayout>




</RelativeLayout>


