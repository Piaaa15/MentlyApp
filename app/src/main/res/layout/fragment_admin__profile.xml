<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin_Profile"
    android:background="@color/white">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="63dp"
        android:layout_height="63dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="100dp"
        android:src="@drawable/fadiyah" />
    <TextView
        android:id="@+id/userProfileAdmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="110dp"
        android:layout_marginTop="105dp"
        android:fontFamily="@font/poppins"
        android:text="Admin"
        android:textColor="@color/black"
        android:textSize="18dp" />
    <TextView
        android:id="@+id/idnama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="110dp"
        android:layout_marginTop="134dp"
        android:fontFamily="@font/poppins"
        android:text="Admin"
        android:textColor="#1E1EAE"
        android:textSize="16dp" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="65dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="34dp"
        android:gravity="center"
        android:fontFamily="@font/poppins"
        android:text="Profile"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txt_pengaturanakun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:text="Pengaturan Akun"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:layout_marginTop="180dp"
        android:layout_marginStart="24dp"
        />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="230dp"
        android:src="@drawable/profile1" />

    <TextView
        android:id="@+id/informasiPribadi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txt_pengaturanakun"
        android:layout_marginStart="55dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/poppins"
        android:text="Informasi Pribadi"
        android:textColor="@color/black"
        android:textSize="18dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="164dp"
        android:layout_marginTop="235dp"
        android:layout_toEndOf="@+id/informasiPribadi"
        android:src="@drawable/icon2" />

    <TextView
        android:id="@+id/keamanan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/informasiPribadi"
        android:layout_marginStart="24dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Lainnya"
        android:textColor="@color/black"
        android:textSize="20dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/keamanan"
        android:layout_marginStart="24dp"
        android:layout_marginTop="15dp"
        android:src="@drawable/bantuan" />

    <TextView
        android:id="@+id/bantuan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/keamanan"
        android:layout_marginStart="55dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/poppins"
        android:text="Bantuan"
        android:textColor="@color/black"
        android:textSize="18dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="19dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView6"
        android:layout_marginStart="231dp"
        android:layout_marginTop="90dp"
        android:layout_toEndOf="@+id/bantuan"
        android:src="@drawable/icon2" />

    <View
        android:id="@+id/button"
        android:layout_width="351dp"
        android:layout_height="45dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="700dp"
        android:background="@drawable/btnkeluar"
        android:textColor="@color/biru_2" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/button"
        android:layout_alignTop="@+id/button"
        android:layout_alignEnd="@+id/button"
        android:layout_alignBottom="@+id/button"
        android:layout_marginStart="134dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="192dp"
        android:layout_marginBottom="3dp"
        android:src="@drawable/keluar" />

    <TextView
        android:id="@+id/txtkeluar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="190dp"
        android:layout_marginTop="708dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Keluar"
        android:textColor="@color/biru_2"
        android:textSize="18dp" />


</RelativeLayout>


