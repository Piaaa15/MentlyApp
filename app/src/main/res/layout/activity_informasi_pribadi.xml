<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Informasi_Pribadi"
    android:background="@color/white">
    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="63dp"
        android:layout_height="63dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="100dp"
        android:src="@drawable/fadiyah" />
    <TextView
        android:id="@+id/nama1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="110dp"
        android:layout_marginTop="105dp"
        android:fontFamily="@font/poppins"
        android:text="User"
        android:textColor="@color/black"
        android:textSize="18dp" />
    <TextView
        android:id="@+id/idnama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="110dp"
        android:layout_marginTop="134dp"
        android:fontFamily="@font/poppins"
        android:text="Pasien"
        android:textColor="#1E1EAE"
        android:textSize="16dp" />

    <ImageView
        android:id="@+id/btn_back6"
        android:layout_width="20dp"
        android:layout_height="18dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/btn_back" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="170dp"
        android:layout_marginTop="22dp"
        android:gravity="center"
        android:fontFamily="@font/poppins"
        android:text="Profile"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txt_pengaturanakun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:text="Informasi Pribadi"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:layout_marginTop="180dp"
        android:layout_marginStart="24dp"
        />


    <TextView
        android:id="@+id/namalengkap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txt_pengaturanakun"
        android:layout_alignParentStart="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/poppins"
        android:text="Nama Lengkap"
        android:textColor="@color/black"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/nama"
        android:layout_width="369dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="258dp"
        android:background="@drawable/btnkeluar"
        android:ems="10"
        android:inputType="text"
        android:paddingStart="15dp"
        android:textColor="@color/biru_2"
        android:textColorHint="#804094A7"
        android:textSize="14sp" />
    <TextView
        android:id="@+id/txt_Email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="Email"
        android:textColor="@color/black"
        android:textSize="18dp"
        android:layout_marginTop="320dp"
        android:layout_marginStart="24dp" />

    <EditText
        android:id="@+id/Email1"
        android:layout_width="369dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="348dp"
        android:background="@drawable/btnkeluar"
        android:ems="10"
        android:inputType="text"
        android:paddingStart="15dp"
        android:textColor="@color/biru_2"
        android:textColorHint="#804094A7"
        android:textSize="14sp" />
    <TextView
        android:id="@+id/txt_umur"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="Umur"
        android:textColor="@color/black"
        android:textSize="18dp"
        android:layout_marginTop="410dp"
        android:layout_marginStart="24dp" />

    <EditText
        android:id="@+id/umur1"
        android:layout_width="369dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="435dp"
        android:background="@drawable/btnkeluar"
        android:ems="10"
        android:inputType="text"
        android:paddingStart="15dp"
        android:textColor="@color/biru_2"
        android:textSize="14sp" />
    <TextView
        android:id="@+id/txt_jenkel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="Jenis Kelamin"
        android:textColor="@color/black"
        android:textSize="18dp"
        android:layout_marginTop="495dp"
        android:layout_marginStart="24dp" />

    <EditText
        android:id="@+id/jenkel1"
        android:layout_width="369dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="520dp"
        android:background="@drawable/btnkeluar"
        android:drawableRight="@drawable/arrow_dropdown"
        android:ems="10"
        android:inputType="text"
        android:paddingStart="15dp"
        android:textColor="@color/biru_2"
        android:textSize="14sp" />

    <Button
        android:id="@+id/ubah"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:text="Ubah"
        android:layout_marginTop="700dp"
        android:layout_marginStart="30dp"
        android:fontFamily="@font/poppins_bold"
        android:textSize="14sp"
        android:textAllCaps="false"
        android:background="@drawable/button_masuk1"

        />


</RelativeLayout>


