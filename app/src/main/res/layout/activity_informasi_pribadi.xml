<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pengguna_Hasil"
    android:background="@color/white">

    <ImageView
        android:id="@+id/btn_back1"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="25dp"
        android:layout_marginStart="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/btn_back" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="65dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="170dp"
        android:layout_marginTop="22dp"
        android:gravity="center"
        android:fontFamily="@font/poppins"
        android:text="Profile"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txt_pengaturanakun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:text="Informasi Pribadi"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:layout_marginTop="180dp"
        android:layout_marginStart="24dp"
        />


    <TextView
        android:id="@+id/namalengkap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txt_pengaturanakun"
        android:layout_alignParentStart="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/poppins"
        android:text="Nama Lengkap"
        android:textColor="@color/black"
        android:textSize="18dp" />
    <EditText
        android:id="@+id/nama"
        android:layout_width="360dp"
        android:layout_height="57dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="258dp"
        android:background="@drawable/btnkeluar"
        android:textColor="@color/biru_2"
        android:text="   nama" />
    <TextView
        android:id="@+id/txt_Email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="Email"
        android:textColor="@color/black"
        android:textSize="18dp"
        android:layout_marginTop="340dp"
        android:layout_marginStart="24dp" />
    <EditText
        android:id="@+id/Email1"
        android:layout_width="360dp"
        android:layout_height="57dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="368dp"
        android:background="@drawable/btnkeluar"
        android:textColor="@color/biru_2"
        android:text="   Email" />
    <TextView
        android:id="@+id/txt_umur"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="Umur"
        android:textColor="@color/black"
        android:textSize="18dp"
        android:layout_marginTop="450dp"
        android:layout_marginStart="24dp" />
    <EditText
        android:id="@+id/umur1"
        android:layout_width="360dp"
        android:layout_height="57dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="475dp"
        android:background="@drawable/btnkeluar"
        android:textColor="@color/biru_2"
        android:text="   Umur" />
    <TextView
        android:id="@+id/txt_jenkel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="Jenis Kelamin"
        android:textColor="@color/black"
        android:textSize="18dp"
        android:layout_marginTop="560dp"
        android:layout_marginStart="24dp" />
    <EditText
        android:id="@+id/jenkel1"
        android:layout_width="360dp"
        android:layout_height="57dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="585dp"
        android:background="@drawable/btnkeluar"
        android:textColor="@color/biru_2"
        android:text="   Jenis Kelamin" />

    <Button
        android:id="@+id/button2"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:text="Ubah"
        android:layout_marginTop="750dp"
        android:layout_marginStart="30dp"
        android:background="@drawable/button_masuk1"

        />


</RelativeLayout>


